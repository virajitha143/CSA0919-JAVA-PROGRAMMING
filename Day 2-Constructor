class Example {
    int a, b;
    Example() {
        a = 10;
        b = 15;
    }
    Example(int a, int b) {
        this.a = a;
        this.b = b;
    }
    void disp() {
        System.out.println("a = " + a + ", b = " + b);
    }
    public static void main(String[] args) {
        Example obj1 = new Example();
        obj1.disp();
        Example obj2 = new Example(20, 25);
        obj2.disp();
    }
}
